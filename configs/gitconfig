[init]
	defaultBranch = master

[user]
	name = hashcat26
	email = dowaitodoratore@gmail.com

[credential]
	helper = manager

[help]
	format = web

[core]
	symlinks = false
	autocrlf = true
	fscache = true

[color]
	interactive = true
	ui = auto

[pull]
	rebase = false

[rebase]
	autosquash = true

[alias]
	cl = clone --verbose
	cm = !git add -A && git commit -m
	lg = log --oneline --graph
	pl = pull origin
	ps = push origin
	rc = rebase --continue
	ru = remote update
	st = status --short --branch

[diff "astextplain"]
	textconv = astextplain

[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
